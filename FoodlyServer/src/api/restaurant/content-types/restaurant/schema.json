{
  "kind": "collectionType",
  "collectionName": "restaurants",
  "info": {
    "singularName": "restaurant",
    "pluralName": "restaurants",
    "displayName": "Restaurant",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "password": {
      "type": "password",
      "required": true,
      "maxLength": 8
    },
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "address": {
      "type": "string"
    },
    "start_date": {
      "type": "datetime"
    },
    "phone_number": {
      "type": "string"
    },
    "image": {
      "allowedTypes": [
        "images",
        "videos"
      ],
      "type": "media",
      "multiple": true
    },
    "notes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::note.note",
      "mappedBy": "restaurant"
    }
  }
}
